{
  "name": "APP_TASK_LIST",
  "main": "electron.cjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "electron": "electron electron.cjs",
    "start": "concurrently \"npm run dev\" \"npm run electron\"",
    "build:electron": "electron-builder",
    "build": "vite build",
    "build-app": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "build": {
    "asar": false,
    "appId": "com.huy.sub-translator",
    "productName": "APP_TASK_LIST",
    "directories": {
      "output": "/Volumes/Elements/youtube/build3"
    },
    "files": [
      "dist/**",
      "electron.cjs",
      "preload.js"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
